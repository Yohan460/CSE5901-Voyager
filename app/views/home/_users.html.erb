<div class="container-fluid">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Phone</th>
      <th>Admin</th>
    </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.fname %></td>
      <td><%= user.lname %></td>
      <td><%= user.phone %></td>
      <td><%= user.admin %></td>
    </tr>
    <% end %>
    </tbody>
  </table>
</div>